{
  "name": "commit-standardizer",
  "version": "0.0.2",
  "description": "A package tha helps you to keep your commit history standardized",
  "main": "index.js",
  "scripts": {
    "test": "echo 'TESTE'",
    "prebuild": "rm -rf dist/",
    "commit": "node scripts/commit.js",
    "build": "rollup -c",
    "generate:docs": "typedoc --mode file --out ./doc ./index.js --allowJs --ignoreCompilerErrors --module commonjs --includeDeclarations --excludeExternals --hideGenerator"
  },
  "files": [
    "dist/bundle.js"
  ],
  "keywords": [
    "commits",
    "standard",
    "semantic"
  ],
  "author": "pedro.lopes@mv.com.br",
  "license": "MIT",
  "publishConfig": {
    "registry": "http://192.168.0.84:8081/repository/npm-snapshots/"
  },
  "dependencies": {
    "prompts": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.1",
    "typedoc": "^0.14.2"
  }
}
